  "scripts": {
    "start": "webpack-dev-server --config=./webpack.config.dev.js",
    "build": "rm -r ./dist && yarn && NODE_ENV=production webpack --config=./webpack.config.prod.js && cp -r ./public/* ./dist",
    "prerelease": "yarn && NODE_ENV=production isProd=no webpack --config=./webpack.config.prod.js && cp -r ./public/* ./dist",
    "prod": "yarn && NODE_ENV=production isProd=yes  webpack  --config=./webpack.config.prod.js && cp -r ./public/* ./dist",
    "test": "jest --watch ./app",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e:test": "start-server-and-test 8081 cy:open",
    "e2e:test:run": "start-server-and-test 8081 cy:run"
  },
